                           {NetworkAddress,$(NoTitle),$(!REG_VT_SZ),"000000000000"},+
                           {ConnectionType,$(NoTitle),$(!REG_VT_DWORD),0},+
                           {NwayForce,$(NoTitle),$(!REG_VT_DWORD),1},+
                           {FlowCtrl,&(NoTitle),$(!REG_VT_DWORD),0},+
                           {8021pqtag,$(NoTitle),$(!REG_VT_DWORD),0},+
