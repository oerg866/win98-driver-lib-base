        Shell $(UtilityInf), IsNetCardAlreadyInstalled, $(BusNum), +
                                                        $(DeviceNum), +
                                                        $(ProductHardware$(Option)$(Sub)Title), +
                                                        $(ProductHardwareName), +
                                                        "YES"
        ifint $($ShellCode) != $(!SHELL_CODE_OK)
            goto ShellCodeError
        endif

        ifstr $($R0) != "NO_ERROR"
            set Error = $($R0)
